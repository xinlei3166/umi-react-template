{
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=1 umi dev",
    "start": "umi dev",
    "build:dev": "cross-env umi build",
    "build": "cross-env UMI_ENV=online umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx && yarn lint:style",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node ./node_modules/@umijs/fabric/dist/verifyCommit.js"
  },
  "lint-staged": {
    "*.{less,css}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "mockjs": "^1.1.0",
    "qs": "^6.10.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-router-breadcrumbs-hoc": "^4.0.1",
    "react-use": "^17.2.4",
    "umi": "3.5.17",
    "antd": "^4.16.13"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.4",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.6.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.15",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.4.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "windicss-webpack-plugin": "^1.4.4",
    "yorkie": "^2.0.0"
  }
}
